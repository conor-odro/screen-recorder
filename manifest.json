{
  "name": "Getting Started Example",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Build an Extension!",
  "permissions": ["activeTab", "tabCapture"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Screen recorder"
  },
  "web_accessible_resources": [
    "src/*"
  ]
}
